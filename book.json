{
    "title": "awesome-papers",
    "description": "dakai's awesome-papers",
    "author": "Dakai",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
        "sidebar": {
            "我的个人博客": "https://andakai.github.io/"
        }
    },
    "plugins": [
         "-lunr", 
         "-search",
         "-sharing",
         "highlight",
         "livereload",
         "search-pro",
         "expandable-chapters-small",
         "splitter",
         "github",
         "hide-element",
         "code",
         "anchor-navigation-ex",
         "tbfed-pagefooter",
         "autotheme",
         "anchors",
         "disqus"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/andakai/awesome-papers"
        },
        "disqus": {
            "shortName": "https-www-dakaian-com-awesome-papers"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "anchor-navigation-ex": {
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right",
            "multipleH1": false,
            "multipleH2": false,
            "multipleH3": false,
            "multipleH4": false,
            "showLevelIcon": false,
            "showLevel": false
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy 版权信息",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size": 2
        },
        "autotheme": {
            "white": [9, 10, 11, 12, 13, 14, 15, 16],
            "sepia": [6, 7, 8, 17, 18, 19],
            "night": [20, 21, 22, 23, 0, 1, 2, 3, 4, 5]
        }
    }
}